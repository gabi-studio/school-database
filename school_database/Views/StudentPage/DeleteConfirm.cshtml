@model School.Models.Student

<h2>Delete Student</h2>
<p>Are you sure you want to delete this student: @Model.StudentFName @Model.StudentLName?</p>


<form method="post" action="/StudentPage/Delete/@Model.StudentId">
    <button type="submit">Confirm Delete</button>
</form>
<a href="/StudentPage/List">Cancel</a>
