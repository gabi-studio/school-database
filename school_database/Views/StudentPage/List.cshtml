@model IEnumerable<School.Models.Student>

<!-- link to the create new  student -->

<div style="margin-bottom: 15px;">
    <a href="/StudentPage/NewStudent" style="color: #080878; font-weight: bold; text-decoration: none;">+ Add New Student</a>
</div>

<!--list of students-->
@foreach (var Student in Model)
{
    
    <div>
        <a href="/StudentPage/Show/@Student.StudentId">@Student.StudentFName @Student.StudentLName</a>
    </div>
}